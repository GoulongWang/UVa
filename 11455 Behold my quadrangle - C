#include<stdio.h>
int main(){
	int n;
	scanf("%d", &n);
	while(n--){
		int side[4], max = 0, sum = 0;
		scanf("%d%d%d%d", &side[0], &side[1], &side[2], &side[3]);
		for(int i = 0; i <= 3; i++) {
			if(side[i] > max) max = side[i];
			sum += side[i];
		}
		
		if(side[0] == side[1] && side[0] == side[2] && side[0] == side[3])
			printf("square\n");
		else if(side[0] == side[1] && side[2] == side[3] || 
            side[0] == side[2] && side[1] == side[3] || 
            side[0] == side[3] && side[1] == side[2])
			printf("rectangle\n");
		else if(max >= sum - max) printf("banana\n");
		else printf("quadrangle\n");
	}
	return 0;	
}
